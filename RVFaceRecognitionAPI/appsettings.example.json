{
  "ConnectionStrings": {
    "Default": "Server=server_address;Database=database_name;User Id=user_name;Password=user_password;Trust Server Certificate=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Key": "secret_key",
    "Issuer": "https://server_address:server_port",
    "Audience": "https://server_address:server_port"
  },
  "AllowedHosts": "*",
  "VueAddress": "http://client_address:client_port"
}
